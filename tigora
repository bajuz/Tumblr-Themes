<script type="text/javascript" language="javascript" src="http://static.tumblr.com/vaqsgdj/tkym9078y/infinitescroll.js"></script>
<script> $(window).load(function () { var $container = $('#content'); $container.imagesLoaded(function () { $container.masonry({ itemSelector: '.post', isAnimated: false, isFitWidth: true, singleMode: true, columnWidth : 300, }); }); $container.infinitescroll({ navSelector  : '#pagination', nextSelector : '#pagination li a.next', itemSelector : '.post', loading: { finishedMsg: 'No more pages to load.', img: 'http://static.tumblr.com/vaqsgdj/s5Pmgyueb/loading_ani.gif' }, bufferPx: 10, debug: false, errorCallback: function(){ $("#infscr-loading").fadeOut(); $("#load").show(); $("#load").html("No more posts").animate({opacity:0.8},500).fadeOut("normal") }}, function (newElements) { var $newElems = $(newElements); $newElems.hide(); $newElems.imagesLoaded(function () { $container.masonry('appended', $newElems, { isAnimated: false, animationOptions: { duration: 100, easing: 'linear', queue: false } }, function () { $newElems.fadeIn('medium'); }); var opts = $container.data('infinitescroll').options; Tumblr.LikeButton.get_status_by_page(opts.state.currPage); }); }); $container.show(); $('#load').click(function(e){ e.preventDefault(); $container.infinitescroll('retrieve'); }); $(function() {  $(window).scroll(function() { if($(this).scrollTop() != 0) { $('#back_to_top').fadeIn(); } else { $('#back_to_top').fadeOut(); } }); $('#back_to_top #up a').click(function() { $('body,html').animate({scrollTop:0},800); }); }); });</script>{/block:IfInfiniteScroll} {/block:IndexPage}

